    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "esdoc": "^1.1.0",
    "esdoc-importpath-plugin": "^1.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^5.1.1",
    "react-dom": "^16.4.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^3.0.7",
